{
    "name": "mhasnainjafri/cpanel",
    "description": "Cpanel functionalities",
    "keywords": [
        "hasnain",
        "cpanel"
    ],
    "homepage": "https://github.com/hasnain/cpanel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Hasnain",
            "email": "mhasnainjafri0099@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MHasnainJafri\\Cpanel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MHasnainJafri\\Cpanel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MHasnainJafri\\Cpanel\\CpanelServiceProvider"
            ],
            "aliases": {
                "Cpanel": "MHasnainJafri\\Cpanel\\CpanelFacade"
            }
        }
    }
}
